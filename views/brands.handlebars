<div class="container jumbotron brands" style="padding-top: 20px;">
    <div class="row">
        {{#each data}}
        <div class="col-lg-12 col-md-6 col-sm-6 card text-center" style="margin-top: 10px; padding-top: 5px;">
            <ul>
                <h5>{{this.brand_name}}</h5>
                <div class="col" style="padding-left: 5px;">
                    <li class="brandDescription" style="padding-top: 10px;">{{this.brand_description}}</li>
                </div>
            </ul>
        </div>
        {{/each}}
    </div>
</div>
<style>
    .brandDescription {
        list-style: none;
    }
</style>